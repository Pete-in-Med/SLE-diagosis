        "    list_clinical_result_A.append(up_r)\n",
        "    up_r = 0 \n",
        "    list_clinical_result_1.append(up_r) # adding the element\n",
        "    i += 1\n",
        "  elif up_r ==\"Y\" :\n",
        "    list_clinical_result_A.append(up_r)\n",
        "    up_r = 1\n",
        "    list_clinical_result_1.append(up_r) # adding the element\n",
        "    i += 1\n",
        "  elif up_r ==\"NI\" :\n",
        "    list_clinical_result_A.append(up_r)\n",
        "    up_r = 0\n",
        "    list_clinical_result_1.append(up_r) # adding the element\n",
        "    i += 1\n",
        "  else :\n",
        "    print('Please check to ensure that the input is \"y\" / \"n\" / \"ni\", and input the data again')\n",
